    "import sys\n",
    "from collections import deque\n",
    "sys.stdin = open('./input.txt')\n",
    "\n",
    "N = int(sys.stdin.readline())\n",
    "\n",
    "dq = deque([i for i in range(1, N+1)])\n",
    "while len(dq) > 1:\n",
    "    k = dq.popleft()\n",
    "    dq.append(dq.popleft())\n",
    "\n",
    "print(dq.pop())"
   ]
  }
