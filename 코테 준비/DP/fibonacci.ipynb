   "source": [
    "import sys\n",
    "sys.stdin = open('./input.txt', 'r')\n",
    "\n",
    "N = int(sys.stdin.readline())\n",
    "\n",
    "if N > 1:\n",
    "    K = [0] * N\n",
    "    K[0], K[1] = 1,1\n",
    "    for i in range(2, N):\n",
    "        K[i] = K[i-1] + K[i-2]\n",
    "    print(K[N-1])\n",
    "elif N == 1:\n",
    "    print(1)\n",
    "else: \n",
    "    print(0)"
   ]
  }
