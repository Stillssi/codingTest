    "def solution(food):\n",
    "    answer = ''\n",
    "    for i in range(1, len(food)):\n",
    "        k = food[i] // 2\n",
    "        answer += str(i) * k\n",
    "    temp = answer[::-1]\n",
    "    answer += '0' + temp\n",
    "    return answer\n",
    "\n",
    "solution([1, 3, 4, 6]) #\t\"1223330333221\""
